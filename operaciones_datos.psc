Algoritmo operaciones_datos
	Definir dato1,dato2 Como Entero
	Escribir 'digite,dato1,dato2'
	Leer dato1
	Leer dato2
	suma <- dato1+dato2
	Escribir 'el resultado de la suma es:',suma
	resta <- dato1-dato2
	Escribir 'el resultado de la resta es:',resta
	multiplicacion <- dato1*dato2
	Escribir 'el resultado de la multiplicacion es:',multiplicacion
	division <- dato1/dato2
	Escribir 'el resultado de la divison es:',division
FinAlgoritmo
